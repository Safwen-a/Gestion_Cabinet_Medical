{% extends 'base.html.twig' %}

{% block title %}DossierMedical{% endblock %}

{% block body %}
    <h1>DossierMedical</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dossier_medical.id }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ dossier_medical.dateCreation ? dossier_medical.dateCreation|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>MaladieChronique</th>
                <td>{{ dossier_medical.maladieChronique }}</td>
            </tr>
            <tr>
                <th>Allergie</th>
                <td>{{ dossier_medical.allergie }}</td>
            </tr>
        </tbody>
    </table>
    {% if is_granted('ROLE_ASSIST') %}

    <a href="{{ path('app_dossier_medical_index') }}">back to list</a>

    <a href="{{ path('app_dossier_medical_edit', {'id': dossier_medical.id}) }}">edit</a>

    {{ include('dossier_medical/_delete_form.html.twig') }}
    {% endif %}
{% endblock %}
